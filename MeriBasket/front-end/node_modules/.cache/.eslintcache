etail\" checked={LoginType === 'Retail'} onChange={() => handleLoginType(\"Retail\")}/><label>Retail Store</label>\n      <form onSubmit={handleLogin}>\n      <br></br>\n        <div>\n        <label>UserID</label>\n        <br></br>\n        <input type=\"text\" name=\"uid\" required placeholder='Enter User ID'></input>\n        </div>\n        <div>\n        <label>Password</label>\n        <br></br>\n        <input type=\"password\" name=\"pass\" required placeholder='Enter password'></input>\n        </div>\n        <div> \n        <button type=\"submit\">Login</button>\n        </div>\n      </form>\n      </>\n      }\n    </div>\n      \n  )\n}\n\nexport default App;\n",[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\reportWebVitals.js",[],[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Orders.js",[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Cart.js",[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Login.js",["69"],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Checkout.js",["70","71"],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Homepage.js",[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Customer\\Profile.js",[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Retail\\ManageOrder.js",["72"],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Retail\\Home.js",[],[],"C:\\Users\\priya_5c9gi56\\OneDrive\\Desktop\\MeriBasket\\front-end\\src\\components\\Retail\\ManageDiscount.js",["73"],[],{"ruleId":"74","severity":1,"message":"75","line":1,"column":8,"nodeType":"76","messageId":"77","endLine":1,"endColumn":12},{"ruleId":"74","severity":1,"message":"